<div class="govuk-main-wrapper govuk-width-container">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-full">
            <div class="govuk-body">
                <h1 class="govuk-heading-xl">Check your email</h1>
                <p>An email has been sent to @Model.SentTo</p>
                <p>Click on the link in the email to return to your assessment.</p>
                <a asp-controller="assessment" class="govuk-button" asp-action="return">Return to assessment</a>
                <p><a href="/">Back to start</a></p>
            </div>
        </div>
    </div>
</div>
