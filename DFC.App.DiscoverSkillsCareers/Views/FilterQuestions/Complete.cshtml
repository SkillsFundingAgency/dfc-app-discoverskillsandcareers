@model FilterQuestionsCompleteResponseViewModel
<div class="govuk-width-container">
    <div class="govuk-grid-row govuk-!-margin-bottom-9 govuk-!-margin-top-8">
        <div class="govuk-grid-column-two-thirds">
            <div class="govuk-panel govuk-panel--confirmation govuk-!-margin-bottom-6">
                <h1 class="govuk-panel__title">
                    Assessment complete
                </h1>
            </div>
            <h2 class="govuk-heading-m">What to do next</h2>

            <p class="govuk-body">Next, you will see some careers you may be interested in. You can answer more questions to refine your results.</p>
            
            <p class="govuk-body govuk-!-margin-bottom-5">Your results are based on your responses and do not reflect the opportunities available in the job market.</p>
            
            <a asp-controller="results" asp-action="roles" role="button" class="govuk-button app-button" asp-route-id="@Model.JobCategoryName">See results</a>
        </div>
    </div>
</div>